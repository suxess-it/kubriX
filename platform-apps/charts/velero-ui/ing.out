              number: 80
        path: /api
        pathType: Prefix
      - backend:
          service:
            name: sx-velero-ui-vui-api
            port:
              number: 80
        path: /ws
        pathType: Prefix
  tls:
  - hosts:
    - vui.lab.suxessit.k8s.cloud.uibk.ac.at
    secretName: velero-ui2-tls
