apiVersion: networking.k8s.io/v1
kind: Ingress
metadata:
  annotations:
    cert.gardener.cloud/purpose: managed
    dns.gardener.cloud/class: garden
    dns.gardener.cloud/dnsnames: vui.lab.suxessit.k8s.cloud.uibk.ac.at
    dns.gardener.cloud/ttl: "180"
  generation: 1
  labels:
  name: sx-velero-ui-vui-ingress2
  namespace: velero-ui
spec:
  ingressClassName: nginx
  rules:
